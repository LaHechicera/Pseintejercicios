Proceso Calcular_promedio_4_numeros
	Definir n1, n2, n3, n4, resultado Como Real;
	Escribir "Primera nota";
	Leer n1;
	Escribir "Segunda nota";
	Leer n2;
	Escribir "Tercera nota";
	Leer n3;
	Escribir "Cuarta nota";
	Leer n4;
	resultado=(n1+n2+n3+n4)/4;
	Escribir "Su promedio es :", resultado;
	
FinProceso
